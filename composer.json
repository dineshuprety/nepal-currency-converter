{
    "name": "dinesh-uprety/nepal-currency-converter",
    "description": "This package is used to convert the rs => other currency.",
    "keywords": [
        "Dinesh Uprety",
        "nepal-currency-converter"
    ],
    "homepage": "https://github.com/dinesh-uprety/nepal-currency-converter",
    "license": "MIT",
    "authors": [
        {
            "name": "dineshuprety",
            "email": "dineshuprety500@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Nepo\\NepalCurrencyConverter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nepo\\NepalCurrencyConverter\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
